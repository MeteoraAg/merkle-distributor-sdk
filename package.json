{
  "name": "@meteora-ag/merkle-distributor-sdk",
  "version": "1.0.0",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "author": "Dann Wee <dann@raccoons.dev>",
  "keywords": [
    "meteora-ag",
    "merkle-distributor"
  ],
  "files": [
    "dist"
  ],
  "description": "A Typescript SDK for interacting with the Merkle Distributor Program on Meteora.",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && tsup src/index.ts --format esm,cjs --dts",
